{
  "name": "tg-resources-root",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "version": "3.3.0",
  "description": "Abstractions on-top of `superagent` (or other Ajax libaries) for communication with REST.",
  "scripts": {
    "lerna": "lerna",
    "create-package": "node scripts/bootstrapPackage.js",
    "bootstrap": "check-engines && lerna bootstrap",
    "build": "lerna --ignore=examples/* exec yarn build",
    "run-examples": "lerna exec --scope=run-examples yarn start",
    "check-formatting": "prettier --check 'packages/**/src/**/*.{ts,tsx}' 'packages/**/test/**/*.{ts,tsx}'",
    "show-formatting": "prettier 'packages/**/src/**/*.{ts,tsx}' 'packages/**/test/**/*.{ts,tsx}'",
    "code-format": "prettier --write 'packages/**/src/**/*.{ts,tsx}' 'packages/**/test/**/*.{ts,tsx}'",
    "lint": "yarn check-formatting && lerna --ignore=examples/* exec yarn lint",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "precoveralls": "yarn test:coverage --no-cache",
    "check-packages": "yarn clean && yarn build && yarn test && yarn lint",
    "bump-version": "lerna --ignore=examples/* version --no-push --exact",
    "prerun-publish": "yarn build",
    "run-publish": "lerna --ignore=examples/* publish",
    "clean": "lerna exec yarn clean && yarn clean-logs",
    "clean-incl-deps": "yarn clean && yarn clean-deps",
    "clean-deps": "rimraf packages/*/node_modules && rimraf yarn.lock && rimraf */**/yarn.lock && rimraf node_modules",
    "clean-logs": "rimraf *.log && rimraf */**/*.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thorgate/tg-resources.git"
  },
  "keywords": [
    "api",
    "request",
    "http",
    "ajax"
  ],
  "author": "Thorgate <info@thorgate.eu>",
  "contributors": [
    "Jürno Ader <jyrno42@gmail.com> (https://github.com/Jyrno42)",
    "Jörgen Ader <jorgen.ader@gmail.com> (https://github.com/metsavaht)",
    "Simon Schmidt (https://github.com/simonschmidt)",
    "Romet Tagobert (https://github.com/amPerl)",
    "Jaak Erisalu (https://github.com/jaakerisalu)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thorgate/tg-resources/issues"
  },
  "homepage": "https://github.com/thorgate/tg-resources#readme",
  "dependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-transform-async-to-generator": "7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-json": "4.0.3",
    "@rollup/plugin-node-resolve": "7.1.2",
    "@rollup/plugin-replace": "2.3.2",
    "@types/body-parser": "^1.17.0",
    "@types/cookie": "^0.3.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/jest": "25.1.3",
    "@types/lodash.template": "^4.4.4",
    "@types/multiparty": "^0.0.32",
    "@types/node": "^13.1.0",
    "@types/qs": "^6.9.5",
    "@types/superagent": "^4.1.6",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "2.19.1",
    "@typescript-eslint/parser": "2.19.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "body-parser": "^1.17.2",
    "check-engines": "1.5.0",
    "cookie-parser": "^1.4.3",
    "debug": "*",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "express": "^4.15.3",
    "form-data": "^3.0.0",
    "fs-extra": "*",
    "isparta": "*",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "jest-extended": "0.11.5",
    "lerna": "3.20.2",
    "lodash": "^4.17.19",
    "lodash.template": "*",
    "multiparty": "*",
    "prettier": "2.1.2",
    "qs": "^6.9.4",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "0.13.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.0",
    "rollup-plugin-typescript2": "0.26.0",
    "superagent": "^5.2.1",
    "temp-dir": "2.0.0",
    "ts-jest": "26.4.4",
    "tslib": "2.0.3",
    "typescript": "3.9.7",
    "uuid": "^7.0.0",
    "watch": "*"
  },
  "engines": {
    "node": ">=10"
  }
}
